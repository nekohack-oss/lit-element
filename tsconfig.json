{
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "target": "es6",
    "lib": ["dom", "esnext"],
    "skipLibCheck": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "moduleResolution": "node"
  }
}
